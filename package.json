{
  "name": "khomeinijs",
  "version": "1.0.0",
  "description": "A library for returning Khomeini quotes",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup index.ts --format cjs, esm --dts --clean",
    "watch": "npm run build -- --watch .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Alireza-Kiani/khomeinijs.git"
  },
  "keywords": [
    "khomeini",
    "template",
    "khomeinijs"
  ],
  "author": "Alireza Kiani <alirezakiaei@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alireza-Kiani/khomeinijs/issues"
  },
  "homepage": "https://github.com/Alireza-Kiani/khomeinijs#readme",
  "devDependencies": {
    "@swc/core": "^1.3.23",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
